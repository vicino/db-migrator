{
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": "off",
        "no-console": "off"
    },
    "overrides": [
        {
            "files": [
                "*.test.mjs"
            ],
            "env": {
                "mocha": true
            },
            "rules": {
                "import/no-extraneous-dependencies": "off"
            }
        }
    ]
}